	"mateapp_counter_nocomments": {
		"prefix": "matecounter_nocomm",
		"body": [
			"import 'package:flutter/material.dart';",
			"",
			"void main() => runApp(MyApp());",
			"",
			"class MyApp extends StatelessWidget {",
			"\t@override",
			"\tWidget build(BuildContext context) {",
			"\t\treturn MaterialApp(",
			"\t\t\ttitle: 'Flutter Demo',",
			"\t\t\ttheme: ThemeData(",
			"\t\t\t\tprimarySwatch: Colors.blue,",
			"\t\t\t),",
			"\t\t\thome: MyHomePage(title: 'Flutter Demo Home Page'),",
			"\t\t);",
			"\t}",
			"}",
			"",
			"class MyHomePage extends StatefulWidget {",
			"\tMyHomePage({Key key, this.title}) : super(key: key);",
			"\t",
			"\tfinal String title;",
			"\t",
			"\t@override",
			"\t_MyHomePageState createState() => _MyHomePageState();",
			"}",
			"",
			"class _MyHomePageState extends State<MyHomePage> {",
			"\tint _counter = 0;",
			"\t",
			"\tvoid _incrementCounter() {",
			"\t\tsetState(() {",
			"\t\t\t_counter++;",
			"\t\t});",
			"\t}",
			"\t",
			"\t@override",
			"\tWidget build(BuildContext context) {",
			"\t\treturn Scaffold(",
			"\t\t\tappBar: AppBar(",
			"\t\t\t\ttitle: Text(widget.title),",
			"\t\t\t),",
			"\t\t\tbody: Center(",
			"\t\t\t\tchild: Column(",
			"\t\t\t\t\tmainAxisAlignment: MainAxisAlignment.center,",
			"\t\t\t\t\tchildren: <Widget>[",
			"\t\t\t\t\t\tText(",
			"\t\t\t\t\t\t\t'You have pushed the button this many times:',",
			"\t\t\t\t\t\t),",
			"\t\t\t\t\t\tText(",
			"\t\t\t\t\t\t\t'$$_counter',",
			"\t\t\t\t\t\t\tstyle: Theme.of(context).textTheme.display1,",
			"\t\t\t\t\t\t),",
			"\t\t\t\t\t],",
			"\t\t\t\t),",
			"\t\t\t),",
			"\t\t\tfloatingActionButton: FloatingActionButton(",
			"\t\t\t\tonPressed: _incrementCounter,",
			"\t\t\t\ttooltip: 'Increment',",
			"\t\t\t\tchild: Icon(Icons.add),",
			"\t\t\t),",
			"\t\t);",
			"\t}",
			"}",
			""
		]
	}
}